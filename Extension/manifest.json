{
    "manifest_version": 3,
    "name": "Wiki-Fly",
    "version": "1.0",
    "description": "Chrome extension that can query wikipedia for a summary of description of a piece of text highlighted by the user on any web page.",
    "permissions": ["activeTab","storage"],
    "icons": {
        "16": "images/WikiFly_icon16.png",
        "32": "images/WikiFly_icon32.png",
        "48": "images/WikiFly_icon48.png",
        "128": "images/WikiFly_icon128.png"
    },
    "action": {
        "default_popup": "popup/popup.html"
      },
    "background": {
        "service_worker": "background.js"
    }
  }